{
  "name": "biola-styleguide",
  "description": "Styleguide for biola's frontend websites",
  "keywords": [
    "css",
    "js"
  ],
  "homepage": "http://biola.github.io/styleguide",
  "license": "MIT",
  "moduleType": "globals",
  "main": [
    "dist/css/biola-styleguide.css",
    "dist/js/biola-styleguide.js"
  ],
  "ignore": [
    "/.*",
